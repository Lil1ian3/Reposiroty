public static String TransformAsmDOB(String input) {
	
	if(input.length() > 20){
	String AsmMth = ""; 
	String AsmDay = "";
	String AsmYr = "";

	
		
	AsmMth = input.substring(4,input.length()- 21); /* gets Month from Wed Sep 08 00:00:00 PDT 1943   - returns Sep */
	AsmDay = input.substring(8,input.length()- 18); /* gets Day from Wed Sep 08 00:00:00 PDT 1943   - returns 08 */
	AsmYr = input.substring(24,input.length()- 0); /* gets Year from Wed Sep 08 00:00:00 PDT 1943   - returns 1943 */
	
	input = AsmDay + "/" + AsmMth + "/" + AsmYr;
	}
	//else { mydate = TalendDate.formatDate("dd/MM/yyyy",input);}
        return input; /* gets Year from Wed Sep 08 00:00:00 PDT 1943   - returns "08-Sep-1943" */
	
    }


