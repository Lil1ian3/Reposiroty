public static String TransformAsmDates(String input) {
	
	String Input1 = "";

	String AsmDtMth = ""; 
	String AsmDtDay = "";
	String AsmDtYr = "";

	
	int AsmVar1 = 0;
	int AsmVar2 = 0;
	int AsmVar3 = 0;
	int AsmVar4 = 0;


	if(input.length() > 20){
		
	AsmDtMth = input.substring(4,input.length()- 21); /* gets Month from Wed Sep 08 00:00:00 PDT 1943   - returns Sep */
	AsmDtDay = input.substring(8,input.length()- 18); /* gets Day from Wed Sep 08 00:00:00 PDT 1943   - returns 08 */
	AsmDtYr = input.substring(24,input.length()- 0); /* gets Year from Wed Sep 08 00:00:00 PDT 1943   - returns 1943 */
	
	input = AsmDtDay + "/" + AsmDtMth + "/" + AsmDtYr;
	}
	else { 
		
		AsmVar1 = input.length();
		/* get month*/
		AsmVar2 = input.indexOf("/");
		AsmDtMth = input.substring(0,AsmVar2);
		
		if (AsmDtMth.length() == 1){
		AsmDtMth = "0" + AsmDtMth;
		}
		
		/*create new date string*/
		AsmVar3 = AsmVar2 + 1;
		Input1 = input.substring(AsmVar3,input.length());
		/* get day*/
		AsmVar4 = Input1.indexOf("/");
		AsmDtDay = Input1.substring(0,AsmVar4);
		
		if (AsmDtDay.length() == 1){
			AsmDtDay = "0" + AsmDtDay;
			}
		/* get year*/
		AsmDtYr = "20" + StringHandling.RIGHT(input,2);
		/* create new date in mm/dd/yyyy */
		input = AsmDtMth + "/" + AsmDtDay + "/" + AsmDtYr;
		
		
		
	}
	//System.out.println(input);
        return input; /* returns "08-Sep-1943" or  "mm/dd/yyyy" */
	
    }
